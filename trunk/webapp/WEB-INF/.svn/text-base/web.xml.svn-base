<?xml version="1.0" encoding="ISO-8859-1"?>

<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN" "http://java.sun.com/j2ee/dtds/web-app_2_3.dtd">

<web-app>
	<listener>
		<!-- Use this one to pull the aws creds from included aws.properties files
		-->
		<listener-class>com.directthought.lifeguard.LifeguardWebStarter</listener-class>
		<!-- Use this one instead to pull creds from global resources defined in server XML
		<listener-class>com.directthought.lifeguard.LifeguardWebStarterEnv</listener-class>
		-->
	</listener>
</web-app>
